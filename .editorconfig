# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
charset = utf-8

[node_modules/**]
end_of_line = unset
insert_final_newline = unset
indent_style = unset
indent_size = unset
trim_trailing_whitespace = unset
